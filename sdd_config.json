{
    "fileTypes": [
        {
            "description": "CSS",
            "fileType": ".css"
        },
        {
            "description": "Comma-separated values",
            "fileType": ".csv"
        },
        {
            "description": "HTML",
            "fileType": ".html"
        },
        {
            "description": "JavaScript",
            "fileType": ".js"
        },
        {
            "description": "JSON",
            "fileType": ".json"
        },
        {
            "description": "Plain text",
            "fileType": ".txt"
        }
    ],
    "scanPatterns": [
        {
            "description": "Email address",
            "pattern": "[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Z|a-z]{2,}"
        },
        {
            "description": "Estonian phone number",
            "pattern": "\\b(\\+?372\\s?)?[3-9](?:[- ]?\\d){6,7}\\b"
        },
        {
            "description": "Estonian personal code",
            "pattern": "\\b[1-6][0-9][0-9][0-1][1-9][0-3][1-9]\\d{4}\\b"
        },
        {
            "description": "The word \"phone number\"",
            "pattern": "\\btel(efon(inumber)?)?\\b"
        },
        {
            "description": "IBAN",
            "pattern": "\\b[A-Z]{2}[0-9]{2}[A-Z0-9]{1,30}\\b"
        }
    ]
}
